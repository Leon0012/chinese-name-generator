{
  "envId": "default",
  "version": "1.0.0",
  "framework": {
    "name": "custom",
    "version": "1.0.0"
  },
  "name": "chinese-name-generator",
  "buildCommand": "npm install && pip install -r requirements.txt",
  "startCommand": "node server.js",
  "env": {
    "PORT": "80",
    "PYTHON_PATH": "python3"
  },
  "region": "ap-shanghai",
  "cache": {
    "paths": [
      "node_modules/**",
      "__pycache__/**"
    ]
  },
  "ignoreFiles": [
    "*.pyc",
    "*.pyo",
    "*.pyd",
    ".git/**",
    ".env"
  ]
}
