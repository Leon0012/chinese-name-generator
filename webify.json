{
  "envId": "default",
  "version": "1.0.0",
  "framework": {
    "name": "nodejs",
    "version": "16.x"
  },
  "name": "chinese-name-generator",
  "buildCommand": "npm install",
  "startCommand": "node server.js",
  "outputPath": ".",
  "env": {
    "PORT": "80",
    "NODE_ENV": "production"
  },
  "region": "ap-shanghai",
  "cache": {
    "paths": [
      "node_modules/**"
    ]
  },
  "ignoreFiles": [
    "*.msi",
    "*.pyc",
    "*.pyo",
    "*.pyd",
    ".git/**",
    ".env",
    "__pycache__/**",
    "*.log",
    "config.js"
  ]
}
