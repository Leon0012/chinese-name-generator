{
  "envId": "default",
  "version": "1.0.0",
  "framework": {
    "name": "custom",
    "version": "1.0.0"
  },
  "name": "chinese-name-generator",
  "buildCommand": "npm ci",
  "startCommand": "npm start",
  "outputPath": ".",
  "env": {
    "PORT": "80",
    "NODE_ENV": "production"
  },
  "region": "ap-shanghai",
  "cache": {
    "paths": [
      "node_modules/**"
    ]
  },
  "ignoreFiles": [
    "*.msi",
    "*.pyc",
    "*.pyo",
    "*.pyd",
    ".git/**",
    ".env",
    "__pycache__/**",
    "*.log",
    "config.js"
  ]
}
